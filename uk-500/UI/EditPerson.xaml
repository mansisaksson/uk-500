<Window x:Class="uk_500.EditPerson"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:uk_500"
        mc:Ignorable="d"
        Title="EditPerson" Height="450" Width="800">
    <Canvas x:Name="MainCanvas">
        <Button Content="Submit" Canvas.Left="10" Canvas.Bottom="10" Width="75" Click="Button_Click"/>
        <Button Content="Cancel" Canvas.Left="100" Canvas.Bottom="10" Width="75" Click="Button_Click_1"/>
    </Canvas>
</Window>
